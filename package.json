{
    "name": "unittest-navigate",
    "displayName": "Unit Test Navigate",
    "description": "Adds `describe`, `it`, `suite`, and `test` calls to the symbols list for navigating javascript and typescript unittest scripts.",
    "version": "0.0.1",
    "publisher": "roblourens",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages",
        "Other"
    ],
    "activationEvents": [
        "onLanguage:typescript",
        "onLanguage:javascript"
    ],
    "main": "./out/src/extension",
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}